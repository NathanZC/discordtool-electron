{
  "name": "DiscordTool",
  "version": "1.0.0",
  "private": true,
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "build": "electron-builder"
  },
  "devDependencies": {
    "electron": "^28.0.0",
    "electron-builder": "^24.9.1",
    "fs-extra": "^11.2.0"
  },
  "build": {
    "appId": "com.example.test",
    "files": [
      "**/*",
      "!node_modules/.bin",
      "!node_modules/electron/**/*",
      "!node_modules/electron-builder/**/*",
      "!**/*.map"
    ],
    "directories": {
      "output": "dist"
    },
    "extraResources": [
      {
        "from": "node_modules/",
        "to": "node_modules/",
        "filter": [
          "electron-store/**/*"
        ]
      }
    ]
  },
  "dependencies": {
    "electron-store": "^8.2.0"
  }
}
